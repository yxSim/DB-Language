(defun select (db arg1 &optional arg2)
  (let ((list (getf db arg1))
        (list2 nil))
    (if arg2
        (progn
          (dolist (list list)
            (setf list2 (append list2 (list (getf list arg2)))))
          list2)
      list)))

(defun where (db arg1 pred &optional arg2)
  (let ((result nil))
    (dolist (value db)      
      (if (listp (first value))
          (if (where value arg1 pred arg2) 
                (setf result (append result (list value))))
        (progn
          (if (funcall pred arg1 (getf value arg2))
              (setf result (append result (list value)))))))
    result))

(defun moreContinentsPredicate (arg1 arg2)
  (if (> (length arg2) arg1)
      t
    nil))

(defun moreContinents (db size)
  (let ((result nil))    
    (dolist (list (where (select db 'COUNTRY) size #'moreContinentsPredicate :CONTINENTS))
      (setf result (append result (list (select list :NAME)))))
    result))

(defun average (list)
  (/ (apply '+ list) (length list)))

(defun myFind (list value)
  (let ((found nil))
    (dolist (l list)
      (if (equalp value l)
          (setf found t)))
    found))

(defun averageArea (continent)
  (let ((area 0)
        (n 0))
    (dolist (country continent)
      (setf area (+ area (getf country :AREA)))
      (setf n (+ n 1)))
    (/ area n)))

(defun getCountriesOnContinent (db continent)
  (let* ((countries (select db 'COUNTRY))
         (countriesOnContinent nil))
    (dolist (country countries)
      (let ((continents (select country :CONTINENTS)))
        (if (myFind continents continent)
              (setf countriesOnContinent (append countriesOnContinent (list country))))))
    countriesOnContinent))

(defun countriesLargerThanAverageArea (db)
  (let ((continents (select db 'CONTINENT :CODE))
        (largerCountries nil))
    (dolist (continent continents)
      (let* ((countriesOnContitent (getCountriesOnContinent db continent))
             (averageArea (averageArea countriesOnContitent)))
        (dolist (country countriesOnContitent)
          (if (> (getf country :AREA) averageArea)
              (setf largerCountries (append largerCountries (list (getf country :NAME))))))))
    largerCountries))
    
(defun largestCountriesOnContinents (db)
  (let ((continents (select db 'CONTINENT :CODE))
        (largestCountries nil))
    (dolist (list (getf db 'CONTINENT) 'CODE)
      (setf continents (append continents (list (getf list :CODE)))))
    (dolist (continent continents)
      (let ((countriesOnContitent (getCountriesOnContinent db continent))
            (maxArea 0)
            (countryOnContinent nil))
        (dolist (country countriesOnContitent)
          (if (> (getf country :AREA) maxArea)
              (progn
                (setf maxArea (getf country :AREA))
                (setf countryOnContinent (getf country :NAME)))))
        (setf largestCountries (append largestCountries (list continent countryOnContinent)))))
    largestCountries))

(defun countriesOnContinent (db)
  (let ((continents (select db 'CONTINENT :CODE))
        (countries nil))
    (dolist (continent continents)
      (setf countries (append countries (list continent)))
      (let ((countriesOnContinent (getCountriesOnContinent db continent))
            (countriesList nil))
        (dolist (country countriesOnContinent)         
          (setf countriesList (append countriesList (list (select country :NAME)))))
        (setf countries (append countries (list countriesList)))))
    countries))

(defun averageDeflectionOfSettlement (db)
  (let ((countries (select db 'COUNTRY))
        (stored (select db 'COUNTRY :POP-DENSITY))
        (calculated nil))
    (dolist (country countries)
      (if (not (= (select country :AREA) 0))
            (setf calculated (append calculated (list (/ (select country :POPULATION) (select country :AREA)))))
        (setf calculated (append calculated (list (select country :POP-DENSITY))))))
    (var stored calculated)))    

(defun var (list1 list2)
  (let ((list nil))
    (dotimes (i (length list1))
      (setf list (append list (list (abs (- (nth i list1) (nth i list2)))))))
    (float (/ (apply '+ list) (length list1)))))


